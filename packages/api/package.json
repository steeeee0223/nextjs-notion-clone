    "@swy/db": "workspace:*",
    "@trpc/server": "catalog:",
    "superjson": "2.2.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@swy/eslint-config": "workspace:*",
    "@swy/prettier-config": "workspace:*",
    "@swy/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@swy/prettier-config"
}
